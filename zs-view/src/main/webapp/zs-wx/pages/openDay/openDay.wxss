.van-icon-close{position: absolute !important;top: 5px;right: 5px;font-size: 20px !important;}
.van-cell{border: 1px solid #eee;margin-bottom: 15px;}
.van-icon{
  color: #008784;
}
.van-button{height: 45px !important;line-height: 45px !important;}
.search-content{padding: 20px;}

.article-nav {
    position: relative;
	    display: flex;
	    flex-direction: row;
	    justify-content: space-between;
		font-size: 28rpx;
    padding: 0rpx 35rpx 30rpx;
	}
  .article-nav::after{content: '';width: 100%;height: 1px;background: #d5d5d5;position: absolute;left: 0;right: 0;bottom: 0;}
	.article-nav-left,.article-nav-right{display: flex;height: 35rpx;line-height: 35rpx;}
	.icon{
		width: 35rpx;height: 35rpx;display: flex;
		flex-direction: row;
		justify-content: center;
		align-items: center;
		margin-right: 8rpx;
	}