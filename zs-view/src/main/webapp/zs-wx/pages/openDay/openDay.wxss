page{background: #f3f3f5;}
.cell{background: #fff;margin-bottom: 30rpx;}
/* .title{font-size: 35rpx;text-align: left;padding: 35rpx 20rpx;border-bottom: 1px solid #e4e4e4;} */
.title{font-size: 35rpx;text-align: left;padding: 35rpx 35rpx;border-bottom: 1px solid #e4e4e4;}
.titlebt{font-size: 35rpx;text-align: left;padding: 40rpx 35rpx 30rpx;}
.content{padding: 20rpx 20rpx;border-bottom: 1px solid #e4e4e4;}
.content p{font-size: 30rpx;color: #474747;}
.article{
		width: calc(100% - 70rpx);
		overflow: hidden;
		line-height: 50rpx;
		font-size: 32rpx;
		color: #474747;
		padding: 30rpx 35rpx;
	}
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  min-height: 60rpx;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: left;
    font-size: 30rpx;
}
.td-name{color: #595959;}
.float-btn2{
  background: #00aba5;
  border: 1px solid #008783;
  color: #fff;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  position: fixed;
  top: 730rpx;
  right: 35rpx;
  font-size: 29rpx;
  padding: 15rpx;
  text-align: center;
  z-index: 100;
}
