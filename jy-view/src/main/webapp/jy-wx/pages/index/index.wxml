<!--index.wxml-->
<swiper class='index-swiper' indicator-dots="true" autoplay="true">
  <block wx:for="{{imgUrls}}" wx:key="{{index}}">
    <swiper-item>
      <image src='{{item}}'></image>
    </swiper-item>
  </block>
</swiper>
<van-grid square>
  <van-grid-item image="{{menuList.image}}" text="{{item.text}}" wx:for="{{ menuList }}"></van-grid-item>
</van-grid>
