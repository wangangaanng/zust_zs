/* pages/xjhXq/xjhXq.wxss */
@import "../../libs/wxParse/wxParse.wxss";

.cell{background: #fff;margin-bottom: 30rpx;}
.title{font-size: 35rpx;text-align: left;padding: 35rpx 20rpx;border-bottom: 1px solid #e4e4e4;}
.content{padding: 20rpx 20rpx;border-bottom: 1px solid #e4e4e4;}
.content p{font-size: 30rpx;color: #474747;}
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  min-height: 60rpx;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: left;
    font-size: 30rpx;
}
.td-name{color: #595959;}
.article{
		width: calc(100% - 70rpx);
		overflow: hidden;
		line-height: 50rpx;
		font-size: 32rpx;
		color: #474747;
		padding: 30rpx 35rpx;
	}

.float-btn1{
  background-color: #fff;
  height: 96rpx;
  width: 96rpx;
  border-radius: 50%;
  position: fixed;
  top: 750rpx;
  right: 35rpx;
  font-size: 26rpx;
  text-align: center;
  z-index: 100;
}
.float-btn1 image{height: 96rpx;
  width: 96rpx;}
