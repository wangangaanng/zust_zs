/**app.wxss**/
.com-black{
  color: #686868;
}
.van-popup--bottom{
  border-radius: 22px 22px 0px 0px;
}
.button {
  padding: 0 70rpx;
  margin-bottom: 80rpx;
}

.button button {
  background-image: linear-gradient(0deg, #51cca3 0%, #26b887 100%);
  box-shadow: 0px 3px 10px 0px rgba(69, 214, 165, 0.75);
  border-radius: 40rpx;
  color: #fff;
  font-size: 16px;
}
.button button:active{opacity: 0.9}
.hiddenInput{display: none}
.main-btn_wrap{padding: 80rpx;margin-bottom: 0px;}
.van-picker__confirm{
  color: #26b887!important;
}
.van-picker__cancel{
  color: #999999!important;
}
.com-wrap{
  padding: 25rpx 30rpx 0;
  background: #FFF;
  color: #333;
  margin-bottom: 20rpx;
  box-shadow: 0px 1px 0px rgba(226, 226, 226, 0.75);
}
.shadow-wrap{
  box-shadow: 0px 1px 0px rgba(226, 226, 226, 0.75);
}
.com-title{position: relative;padding-left: 20rpx;font-size: 30rpx;}
.com-title:before{position: absolute;left: 0px;content: "";width: 6rpx;height: 40rpx;background: rgb(38,184,135);
border-radius: 3px;}
.sm-title{font-size: 30rpx;}
.btn-upload{border: 1px dotted rgb(38,184,135);border-radius: 10rpx;text-align: center;height:288rpx;margin: 15px 20px 0px 20px;}
.btn-upload .icon-hao{font-size: 60rpx;color: rgb(38,184,135);}
.btn-upload image{width: 100%;height: 288rpx;border-radius: 4px;}
.btn-upload_wrap{padding-top: 84rpx;}
.btn-txt{font-size: 28rpx;color:rgb(38,184,135); }
.cell-wrap{padding: 30rpx 0rpx;}
.cell-wrap input{ text-align: right}
.unit{color: #6d6d6d;padding-left: 6px;}
.bottom-txt{color: #7b7b7b;font-size: 26rpx;text-align: center;position: absolute;bottom: 10%;width: 100%;}
.icon-tongzhi{color:rgb(231,31,25);font-size:44rpx;margin-right:20rpx;}
.notice-detail{font-size: 24rpx;line-height: 36rpx;color: rgb(235,24,24);}
/* 下划线 */
.line-btm{position: relative;}
.line-btm::after{position: absolute;box-sizing: border-box;-webkit-transform-origin: center;
transform-origin: center;content: " ";pointer-events: none;top: auto;right: 0;bottom: 0;left: 0px;
border-bottom: 1px solid #eee;-webkit-transform: scaleY(.5);transform: scaleY(.5);}
.bottom-btn_wrap{padding: 65rpx 30rpx 65rpx;box-sizing: border-box;}
.bottom-btn_wrap button{background: #26b887;height: 88rpx;line-height: 88rpx;font-size: 30rpx;}
.bottom-btn_wrap .main-button:nth-child(2){margin-left: 40rpx;}
.van-cell__title{color: #333;font-size: 30rpx;}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.null-content{
  text-align: center;
  font-size: 28rpx;
}

.null-content image{
  width: 120px;
  height: 120px;
}

.null-content view{
  margin-top: 10px;
  color: #999;
}
.error-tip .iconfont{font-size: 38rpx;margin-right: 5rpx;position: absolute;left: 30rpx;top: -10rpx;}
.error-tip {padding: 4rpx 30rpx 4rpx 60rpx;background: rgba(255, 0,0, 0.1);position: relative;border-radius: 10px;margin-top: 20rpx;}
.red{color: #FF0000}
.df{display: flex}
.f1{flex: 1}
.fxs{font-size: 24rpx;}
.fs{font-size: 26rpx;}
.fm{font-size: 28rpx;}
.fl{font-size: 30rpx;}
.p15{padding: 30rpx;}
.color9{color: #999;}
.mb10{margin-bottom: 20rpx;}
.mt10{margin-top: 10px;}
.tc{text-align: center}
.bk-grey{background: #f9f9f9;}
.bk-white{background: #FFF;}

