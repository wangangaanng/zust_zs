/**app.wxss**/
.com-black{
  color: #686868;
}
.van-popup--bottom{
  border-radius: 22px 22px 0px 0px;
}
.main-btn_wrap{
  padding: 90rpx;
}
.main-btn_wrap button{
  background-image: linear-gradient(0deg, rgb(81,204,163) 0%, rgb(38,184,135) 100%);
  box-shadow: 0px 3px 10px 0px rgba(69, 214, 165, 0.75);
  height: 80rpx;
  line-height: 80rpx;
}
.van-picker__confirm{
  color: #26b887!important;
}
.van-picker__cancel{
  color: #999999!important;
}
.com-wrap{
  padding: 25rpx 30rpx 0;
  background: #FFF;
  color: #333;
  margin-bottom: 20rpx;
}
.cell-wrap{padding: 30rpx 0rpx;}
.cell-wrap input{ text-align: right}
.unit{color: #6d6d6d;padding-left: 6px;}

/* 下划线 */
.line-btm{position: relative;}
.line-btm::after{position: absolute;box-sizing: border-box;-webkit-transform-origin: center;
transform-origin: center;content: " ";pointer-events: none;top: auto;right: 0;bottom: 0;left: 0px;
border-bottom: 1px solid #eee;-webkit-transform: scaleY(.5);transform: scaleY(.5);}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.null-content{
  text-align: center;
  font-size: 28rpx;
}

.null-content image{
  width: 120px;
  height: 120px;
}

.null-content view{
  margin-top: 10px;
  color: #999;
}
.error-tip .iconfont{font-size: 38rpx;margin-right: 5rpx;position: absolute;left: 30rpx;top: -10rpx;}
.error-tip {padding: 4rpx 30rpx 4rpx 60rpx;background: rgba(255, 0,0, 0.1);position: relative;border-radius: 10px;margin-top: 20rpx;}
.red{color: #FF0000}
