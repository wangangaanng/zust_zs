<span style='color:#7f0055; '>&lt;</span><span style='color:#7f0055; '>panel</span> title=<span style='color:#2a00ff; '>"</span><span style='color:#2a00ff; '>Panel Success</span><span style='color:#2a00ff; '>"</span> sclass=<span style='color:#2a00ff; '>"</span><span style='color:#2a00ff; '>panel-success</span><span style='color:#2a00ff; '>"</span><span style='color:#7f0055; '>></span>
	<span style='color:#7f0055; '>&lt;</span><span style='color:#7f0055; '>panelchildren</span><span style='color:#7f0055; '>></span>Panel content<span style='color:#7f0055; '>&lt;/</span><span style='color:#7f0055; '>panelchildren</span><span style='color:#7f0055; '>></span>
<span style='color:#7f0055; '>&lt;/</span><span style='color:#7f0055; '>panel</span><span style='color:#7f0055; '>></span>

<span style='color:#7f0055; '>&lt;</span><span style='color:#7f0055; '>panel</span> title=<span style='color:#2a00ff; '>"</span><span style='color:#2a00ff; '>Panel Info</span><span style='color:#2a00ff; '>"</span> sclass=<span style='color:#2a00ff; '>"</span><span style='color:#2a00ff; '>panel-info</span><span style='color:#2a00ff; '>"</span><span style='color:#7f0055; '>></span>
	<span style='color:#7f0055; '>&lt;</span><span style='color:#7f0055; '>panelchildren</span><span style='color:#7f0055; '>></span>Panel content<span style='color:#7f0055; '>&lt;/</span><span style='color:#7f0055; '>panelchildren</span><span style='color:#7f0055; '>></span>
<span style='color:#7f0055; '>&lt;/</span><span style='color:#7f0055; '>panel</span><span style='color:#7f0055; '>></span>

<span style='color:#7f0055; '>&lt;</span><span style='color:#7f0055; '>panel</span> title=<span style='color:#2a00ff; '>"</span><span style='color:#2a00ff; '>Panel Warning</span><span style='color:#2a00ff; '>"</span> sclass=<span style='color:#2a00ff; '>"</span><span style='color:#2a00ff; '>panel-warning</span><span style='color:#2a00ff; '>"</span><span style='color:#7f0055; '>></span>
	<span style='color:#7f0055; '>&lt;</span><span style='color:#7f0055; '>panelchildren</span><span style='color:#7f0055; '>></span>Panel content<span style='color:#7f0055; '>&lt;/</span><span style='color:#7f0055; '>panelchildren</span><span style='color:#7f0055; '>></span>
